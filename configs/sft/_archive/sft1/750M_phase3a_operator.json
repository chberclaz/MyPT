{
  "name": "GPT-750M-Phase3a-Operator",
  "description": "Operator learning test: COPY, WRAP, EXTRACT with unique payloads. Tests abstract operator generalization.",
  
  "batch_size": 16,
  "block_size": 512,
  "vocab_size": 50304,
  "n_embd": 1280,
  "n_head": 20,
  "n_layer": 32,
  "dropout": 0.08,
