  "warmup_iters": 100,
  "use_amp": true,
  "amp_dtype": "bf16",
  
  "use_loss_mask": true,
  
  "batch_sampling_mode": "epoch",
  "pad_token_id": null,
  "episode_min_tokens": 10,
  "epoch_seed": 7777,
  "epoch_shuffle": true,
  "epoch_drop_last": true,
  
  "weight_decay": 0.05,
  
  "eval_sets": null,
  "eval_seed": 1337,
  "log_file": "logs/train/phase3a_operator.jsonl"
